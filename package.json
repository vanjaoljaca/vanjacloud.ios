{
  "name": "vanjacloud.ios",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "update-phone": "LOCAL_BUILD=true eas update --channel preview",
    "dev": "LOCAL_BUILD=true yarn ios",
    "install-simulator": "eas build --profile dev-simulator --platform ios",
    "install-phone": "eas build --profile preview --platform ios"
  },
  "dependencies": {
    "@config-plugins/react-native-webrtc": "^7.0.0",
    "@eva-design/eva": "^2.1.1",
    "@expo/webpack-config": "^18.1.1",
    "@notionhq/client": "^2.2.3",
    "@react-native-community/datetimepicker": "6.2.0",
    "@react-navigation/material-bottom-tabs": "^6.2.16",
    "@react-navigation/native": "^6.1.7",
    "@shoutem/ui": "^6.1.2",
    "@types/react": "~18.2.14",
    "axios": "^1.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "^49.0.0",
    "expo-av": "~13.4.1",
    "expo-background-fetch": "~11.3.0",
    "expo-build-properties": "~0.8.3",
    "expo-camera": "~13.4.2",
    "expo-dev-client": "~2.4.6",
    "expo-device": "~5.4.0",
    "expo-file-system": "^15.4.2",
    "expo-status-bar": "~1.6.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.10",
    "global": "^4.4.0",
    "ix": "^5.0.0",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "5.1.4",
    "moment": "^2.29.4",
    "openai": "^4.0.0-beta.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.0",
    "react-native-device-info": "9.0.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-http-bridge": "^0.6.1",
    "react-native-http-bridge-refurbished": "^1.2.9",
    "react-native-linear-gradient": "2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "6.2.0",
    "react-native-path": "^0.0.5",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-svg": "12.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-uuid": "^2.0.1",
    "react-native-vision-camera": "^3.8.2",
    "react-native-web": "~0.19.6",
    "react-native-webrtc": "^118.0.1",
    "react-native-webview": "11.23.0",
    "shortid": "^2.2.16",
    "typescript": "^5.1.3",
    "vanjacloud.shared.js": "github:vanjaoljaca/vanjacloud.shared.js#main"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "react-native-dotenv": "^3.4.9"
  },
  "expo": {
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        },
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone."
        },
        "@config-plugins/react-native-webrtc"
      ]
    ]
  }
}
